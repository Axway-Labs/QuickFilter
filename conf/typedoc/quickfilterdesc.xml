<entityStoreData xmlns="http://www.vordel.com/2005/06/24/entityStore">
	<entityType extends="NamedTopLevelGroup" name="QuickFilterGroup">
		<constant name="_version" type="integer" value="1" />
		<componentType cardinality="*" name="QuickFilterDefinition"/>
	</entityType>
	<entityType extends="Entity" name="QuickFilterDefinition">
		<constant name="_version" type="integer" value="1" />
		
		<!-- Name of filter type -->
		<field isKey="true" name="name" type="string"/>
		
		<!-- Name and description that will be displayed in the filter header -->
		<field name="displayName" type="string"/>
		<field cardinality="1" name="description" type="string"/>
		
		<!-- icon and palette category of filter -->
		<field cardinality="1" name="icon" type="string"/>
		<field cardinality="1" name="palette" type="string"/>
		
		<!-- resources properties file -->
		<field cardinality="1" name="resources" type="string"/>
		
		<!-- script language and implementation -->
		<field cardinality="1" default="js" name="engineName" type="string"/>
		<field cardinality="1" name="script" type="string"/>

		<!-- declarative xml and associated resources for filter ui -->
		<field cardinality="1" name="ui" type="string"/>
	</entityType>
	<entityType abstract="true" extends="Filter" name="QuickFilter">
		<constant name="_version" type="integer" value="1" />
		<constant name="class" type="string" value="com.vordel.client.ext.filter.quick.QuickFilter" />
	</entityType>
</entityStoreData>